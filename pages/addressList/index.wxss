.list{
  background: #FFF;
  padding-top: 10px;
  padding-bottom: 10px;
  margin-bottom: 20px;
}
.list .item{
  display: flex;
  margin: 0 18px;
 
  padding: 15px 0;
  align-items: center;
  height: 98px;
}
.item .left{
  flex: 1 0 80%;
  font-size: 14px;
}
.item .edit,.choice{
  flex: 1 0 10%;
  text-align: center;
}
.item .edit image{
 
  width: 20px;
  height: 20px;
}
.item .choice image{
  width: 20px;
  height: 20px;
 display: block;
}
 .choice{
 height: 60px;
  width: 20px;
 
}
.dizhi{
  color: #969696;
  font-size: 16px;
}
.xingm{
  font-size: 18px;
}



.btn{
  height: 8vh;
  line-height: 8vh;
  text-align: center;
  background:green;
  font-size: 14px;
  color: #FFF;
 
 
}