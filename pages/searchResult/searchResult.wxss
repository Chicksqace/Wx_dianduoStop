page{
  height: 100%;
}
.container{
  width: 100%;
  height: 90%;
}
.cs{
  width:126rpx;height:126rpx;position: fixed;bottom: 374rpx;right: 30rpx;z-index: 99998;
}
.cs_but{
  width:126rpx;height:126rpx;position: fixed;bottom: 374rpx;right: 30rpx;z-index: 99999;
  opacity:0;
}
.find{
  display: flex;
  margin: 0 auto;
  justify-content:space-between;
  width: 93%;
  border-bottom:1px solid rgba(0, 0, 0, 0.082); 
 padding-bottom: 10px;
}
.yinying{
   margin-top: 30rpx;
  width: 100%;
  height: 16rpx;
  background-color: #B0B2BF;
  opacity: 0.15;
}
.list{
  
  display: flex;
  justify-content: space-between;  
  flex-wrap:wrap;
  box-sizing: content-box;
}
.list .item{
  flex:0 1 50%;
  /* border-bottom:1px solid #EDEDED;  */
  padding-top: 15px;
}
.list .item:nth-child(odd){
  /* border-right:1px solid #EDEDED;  */
  margin-right: -1px;
}

.list .item .text{
  display: block;
  font-size: 32rpx;
  line-height: 90rpx;
  margin:0 12px;
  color:#969696 ;
}

.list .item .product{
  display: block;
  width: 335rpx;
  height: 480rpx;
  margin: 0 auto;
}
.price{
  display: flex;
  margin:0 10px;
  justify-content:space-between;
}

.price text{
  font-size: 36rpx;
  color: #000;
}

.price image{
  display: block;
  width: 20px;
  height: 20px;
  padding-top: 2px;
}

.color{
 display: block;
 float: right;
  width: 30rpx; height:30rpx;
  margin-right: 10rpx;
  margin-top: 10rpx;
  border: 1px #000 solid;
  

}

.empty_body{
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  position: relative;
  z-index: 50000;
}
.empty_body_img {
  width:248rpx ;height:248rpx ;position: absolute;top:251rpx;left: 251rpx; 
}
.empty_body_text{position: absolute;top: 520rpx;left: 285rpx;display:flex;align-items:center;justify-content:center;}
.empty_body_text text{font-family: PingFangSC-Regular;font-size: 30rpx;color: #969696;letter-spacing: 0;text-align: center;line-height: 56rpx;}
.empty_back{position: absolute;top: 624rpx;left: 303rpx;width: 144rpx;height: 64rpx;display:flex;align-items:center;justify-content:center;background-color: #000000}
.empty_back text{font-family: PingFangSC-Regular;font-size: 26rpx;color: #FFFFFF;letter-spacing: 0;text-align: center;}
#isHidden{
  display: none;
}