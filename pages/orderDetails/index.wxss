
.main{
  height: 82vh;
}

.tab{
  height: 7vh;
  display: flex;
  line-height: 7vh;
}

.tab view{
  flex: 1 0 auto;
  font-size: 14px;
  color: #333333;
  text-align: center;
}
.tab .active{
  color: #ff5f19;
  border-bottom: 1px solid #ff5f19;
  padding-bottom:1.8vh;
}

.list{
  height: 90vh;
  background: #F6F6F6;
}

.list .item{
  display: flex;
 background: #FFF;
  padding: 0 16px;
  height: 115px;
  align-items: center;
  margin-top: 15px;
}

.list .label{
  flex: 0 0 auto;
  padding:10px 0;
}

.list .content{
  flex-grow: 1;
  display:flex;
}

.list .content image{
  flex: 0 0 auto;
  height: 115px;
  width: 78px;
  padding-right:10px;
}

.list .content .text{
  flex-grow: 1;
  font-size: 14px;
}

.list .content .title{
  overflow: hidden;
}

.list .content .spec{
  font-size: 12px;
  color: #333333;
  overflow: hidden;
  text-overflow: ellipsis;
  display: flex;
  line-height: 24px;    
  max-height: 24px;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  margin-top: 10px;
}

.list .content .price{
  display: flex;
  font-size: 16px;
}

.list .content .price text:nth-child(1){
  color: #ff5f19;
  flex: 1 0 50%;
}

.list .content .price text:nth-child(2){
  color: #999999;
  flex: 1 0 50%;
  text-align: right;
}



