.tab{
  height: 7vh;
  display: flex;
  line-height: 7vh;
}

.tab view{
  flex: 1 0 auto;
  font-size: 14px;
  color: #333333;
  text-align: center;
}
.tab .active{
  color: #ff5f19;
  border-bottom: 1px solid #ff5f19;
  padding-bottom:1.8vh;
}
.list{
  height: 71vh;
}

.item{
  display: flex;
  
  height: 100px;
  margin: 10px 10px 0 10px;
  background: #F6F6F6 ;
}

.item .left{
  flex: 0 1 25%;
  text-align: center;
  
  font-size: 18px;
  margin: 28px 0;
  color: #9D3737;
  border-right: 2px dashed #969696;
  margin-right: 8px;
}
.item .left text{
  font-size: 20rpx;
  color: #9D3737 ;
}

.item .right{
  flex: 1 1 auto;
  font-size: 12px;
  color: #999999;
  margin-top: 30px;
  margin-left: 10px;
}
.item .right view{
  line-height: 24px;
}

.item .right text{
  color: #9D3737 ;
}
.state{margin-top: 10px;
margin-left:-10px; 
}
.find{
  display: flex;
  margin:0 10px;
  justify-content:space-between;
  width: 93%;
  margin: 15px auto;
  border-bottom:1px solid rgba(0, 0, 0, 0.082); 
 padding-bottom: 10px;
}

.find text{
  font-size: 28rpx;
  color: #969696;
}

.find image{
  display: block;
  width: 20px;
  height: 20px;
  padding-top: 2px;
}
.chenge{background: #000;
color: #FFF;
font-size: 13px;
width: 72px;
height: 32px;
line-height: 32px;
text-align: center;
}
.find .cn{
  font-size: 16px;
}
.btn{
  height: 8vh;
  line-height: 8vh;
  text-align: center;
  background:#FFF;
  font-size: 14px;
  color: #969696;
 
 
}
